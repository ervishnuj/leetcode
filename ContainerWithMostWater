class ContainerWithMostWater {
    public int maxArea(int[] height) {
        int start=0;int end=height.length-1;
        int result=0;
        while(start<end){
            int minValue=Math.min(height[start],height[end]);
            if(minValue*(end-start)>result){
                result=minValue*(end-start);
            }
            if(height[start]<height[end]){
                start++;
            }else end--;
        }
        return result;
    }
}
