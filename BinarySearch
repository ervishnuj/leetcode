class BinarySearch {
    public int search(int[] nums, int target) {
        int result=-1;
        int i=0;
        int j=nums.length-1;
        while(i<=j){
            int mid=(j+i)/2;
            if(nums[mid]==target)return mid;
            else if(nums[i]==target)return i;
            else if(nums[j]==target)return j;
            else if(nums[mid]>target){
                j=mid-1;
            }else{
                i=mid+1;
            }
        }
        return result;
    }
}
