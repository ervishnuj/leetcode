class ExcelSheetColumnNumber
 {
    public int titleToNumber(String s) {
        int result=0;
        char[] ch=s.toCharArray();
        int j=s.length()-1;
        int inc=1;
        for(int i=0;i<s.length();i++){
            if(i==0)
            result+=(ch[j]-64);
            else
            result+=(inc*(ch[j]-64));
            j--;
            inc*=26;
        }
        return result;
    }
}
