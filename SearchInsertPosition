class SearchInsertPosition {
    public int searchInsert(int[] nums, int target) {
        int result=0;
        int arraysize=nums.length;
        for(int i=0;i<arraysize;i++){
            if(nums[i]>=target){
                return result;
            }else{
                result++;
            }
        }
        return result;
// two pointer using code
/*
 int arraysize=nums.length;
        int start=0;
        int end=arraysize-1;
        while(start<=end){
            int mid=start+((end-start)/2);
            if(nums[mid]==target){
                return mid;        
            }else if(target>nums[mid]){
                start=mid+1;
            
            }else{
                end=mid-1;
            }
        }
        return start;
*/
    }
}
