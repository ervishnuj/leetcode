class AddString {
    public String addStrings(String num1, String num2) {
        int n=num1.length()-1;
        int m=num2.length()-1;
        String s="";
        int balance=0;
        while(n>=0||m>=0){
            int sum=balance;
            if(n>=0)sum+=num1.charAt(n)-'0';
            if(m>=0)sum+=num2.charAt(m)-'0';
            balance=sum>9?1:0;
            s=sum%10+s;
            n--;m--;
        }
        if(balance>0)return 1+s;
        return s;
    }
}
