class BestTimetoBuyandSellStock {
    public int maxProfit(int[] prices) {
        int minPrice=Integer.MAX_VALUE;
        int maxPrice=0;
        for(int price:prices){
            if(minPrice>price){
                minPrice=price;
            }else{
                maxPrice=Math.max(maxPrice,price-minPrice);
            }
        }
        return maxPrice;
    }
}
