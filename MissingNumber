class MissingNumber {
    public int missingNumber(int[] nums) {
        boolean[] b=new boolean[nums.length];
        for(int i=0;i<nums.length;i++){
            if(nums[i]<nums.length)b[nums[i]]=true;
        }
        for(int i=0;i<b.length;i++){
            if(!b[i])return i;
        }
        return b.length;
    }
}
